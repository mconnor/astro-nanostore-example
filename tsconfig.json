{
    "compilerOptions": {
        "allowJs": true,
        "jsx": "preserve",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "jsxImportSource": "preact",
        "skipLibCheck": true,
        "baseUrl": "./",
        "paths": {
            "react": ["./node_modules/preact/compat/"],
            "react-dom": ["./node_modules/preact/compat/"]
        }
    },
    "extends": "astro/tsconfigs/strict"
}
